{
  "name": "youth-hockey-stat-tracker",
  "version": "0.2.3",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "kill-ports": "npx kill-port 4000 && npx kill-port 4500 && npx kill-port 5000 && npx kill-port 8080 && npx kill-port 9099 && npx kill-port 9150 && npx kill-port 9199",
    "export-data": "rm seed-data -r -f && firebase emulators:export seed-data",
    "local-dev": "firebase emulators:start --import seed-data",
    "get-indexes": "firebase firestore:indexes",
    "build:prod": "npm --no-git-tag-version version patch && node ./setBuildDate.js && ng build --configuration=production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "update-angular": "ng update @angular/cli @angular/core @angular/material @angular/cdk"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.11",
    "@angular/cdk": "^18.2.12",
    "@angular/common": "^18.2.11",
    "@angular/compiler": "^18.2.11",
    "@angular/core": "^18.2.11",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^18.2.11",
    "@angular/material": "^18.2.12",
    "@angular/platform-browser": "^18.2.11",
    "@angular/platform-browser-dynamic": "^18.2.11",
    "@angular/router": "^18.2.11",
    "bootstrap": "^5.3.3",
    "buffer": "^6.0.3",
    "exceljs": "^4.4.0",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.7.0",
    "replace-in-file": "^8.2.0",
    "tslib": "^2.8.1",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular/cli": "^18.2.11",
    "@angular/compiler-cli": "^18.2.11",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.9.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2"
  }
}
